<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane id="AnchorPane" prefHeight="492.0" prefWidth="853.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="tn.leaguestorm.gui.AddCategoryController">
   <children>
      <Button fx:id="btnAdd" layoutX="54.0" layoutY="322.0" mnemonicParsing="false" onAction="#saveCategory" prefHeight="25.0" prefWidth="73.0" text="Add" />
      <Label layoutX="41.0" layoutY="157.0" text="Category Name" />
      <Label layoutX="41.0" layoutY="202.0" prefHeight="45.0" prefWidth="102.0" text="Image" />
      <TextField fx:id="tfNom" layoutX="156.0" layoutY="153.0" />
      <TextField fx:id="tfImage" layoutX="156.0" layoutY="212.0" />
      <Button fx:id="btnUpdate" layoutX="139.0" layoutY="322.0" mnemonicParsing="false" onAction="#UpdateCategory" prefHeight="25.0" prefWidth="73.0" text="Update" />
      <Button fx:id="btnDelete" layoutX="224.0" layoutY="322.0" mnemonicParsing="false" onAction="#DeleteCategory" prefHeight="25.0" prefWidth="73.0" text="Delete" />
      <TableView fx:id="categoryTable" layoutX="398.0" layoutY="88.0" onSort="#categories" prefHeight="304.0" prefWidth="427.0">
        <columns>
          <TableColumn fx:id="nameColumn" prefWidth="203.0" text="Category Name" />
          <TableColumn fx:id="imageColumn" prefWidth="223.0" text="Image" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
